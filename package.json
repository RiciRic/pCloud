{
  "name": "pCloud",
  "version": "1.0.0",
  "description": "Password Manager",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder --x64",
    "gh-publish": "electron-builder build --win --publish always"
  },
  "repository": {
    "type" : "git",
    "url" : "https://github.com/RiciRic/pCloud.git"
  },
  "keywords": [
    "Password",
    "Manager",
    "Sick",
    "idk",
    "pCloud"
  ],
  "author": "RiciRic",
  "license": "CC0-1.0",
  "build": {
    "appId": "com.pCloud",
    "mac": {
      "category": "com.electron.pCloud"
    },
    "win": {
